{
  "name": "dein",
  "private": true,
  "packageManager": "yarn@3.3.0",
  "scripts": {
    "build:dns": "yarn workspace dns run build",
    "build:goc": "yarn workspace goc run build",
    "build:all": "yarn workspaces foreach -ptR --from '{dns,goc}' run build"
  },
  "workspaces": [
    "dns-frontend",
    "goc-frontend"
  ]
}
